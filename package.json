{
  "name": "tabs-saver",
  "version": "1.0.0",
  "description": "client, extension and server for tabs-saver",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "extension": "npm start --prefix extension",
    "install-all": "concurrently \"cd client && npm install\" \"cd server && npm install\" \"cd extension && npm install\"",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renemroger/tabs-saver.git"
  },
  "author": "Rene Roger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/renemroger/tabs-saver/issues"
  },
  "homepage": "https://github.com/renemroger/tabs-saver#readme",
  "dependencies": {
    "concurrently": "^4.1.2"
  }
}
